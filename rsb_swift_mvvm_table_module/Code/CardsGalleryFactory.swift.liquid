import TableViewTools

final class CardsGalleryFactory {

    static func makeSectionItems(cellModels: [CardTableViewCellModel]) -> [TableViewSectionItem] {
        let cellItems = cellModels.map(CardTableViewCellItem.init)
        let sectionItem = TableViewSectionItem(cellItems: cellItems)
        return [sectionItem]
    }
}
