- (NSArray *)cellItemsWithCollectionViewSectionItems:(NSArray<id<RSBCollectionViewSectionItemProtocol>> *)sectionItems {
    NSMutableArray *cellItems = [NSMutableArray array];
    for (id<RSBCollectionViewSectionItemProtocol> sectionItem in sectionItems) {
        {{ prefix }}{{ module_info.name }}BaseTableViewCellItem *cellItem = [self.assemblyTableViewCellItem{{ module_info.name }}Module cellItem{{ module_info.name }}Module];
        cellItem.sectionItem = sectionItem;
        [cellItems addObject:cellItem];
    }
    return [NSArray arrayWithArray:cellItems];
}

